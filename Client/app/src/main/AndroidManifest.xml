<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.example.marvelcharacters">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/marvel_characters"
            android:label="@string/app_name"
            android:roundIcon="@drawable/marvel_characters"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning">
        <activity
                android:name=".Favorites"
                android:label="@string/favorites">
        </activity>
        <activity
                android:name=".Creators"
                android:label="@string/creators">
        </activity>
        <activity
                android:name=".HomeScreen"
                android:label="@string/home_screen">
        </activity>
        <activity
                android:name=".Reg"
                android:label="@string/title_reg">
        </activity>
        <activity
                android:name=".Login"
                android:label="@string/title_login">
        </activity>
        <activity
                android:name=".Comics"
                android:label="@string/comics">
        </activity>
        <activity
                android:name=".About"
                android:label="@string/title_about">
        </activity>
        <activity
                android:name=".HomeAdmin"
                android:label="@string/title_admin">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".CharacterActivity">
        </activity>
    </application>

</manifest>